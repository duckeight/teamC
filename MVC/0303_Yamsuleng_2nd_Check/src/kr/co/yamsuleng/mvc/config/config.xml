<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<!-- mybatis사용 시, DTO 역할인 UploadDemoVO의 별칭을 upvo 라고 주는 방법 -->
	<!-- root-context로 가서 별칭이 사용될 수 있게 세팅해준다 -->
	<typeAliases>
		<typeAlias type="kr.co.yamsuleng.mvc.dto.Criteria"  alias="crivo"/>
		
		<typeAlias type="kr.co.yamsuleng.mvc.dto.AccountVO"  alias="accvo"/>
		
		<typeAlias type="kr.co.yamsuleng.mvc.dto.PostVO"  alias="pvo"/>
		
		<typeAlias type="kr.co.yamsuleng.mvc.dto.HashTagRelVO"  alias="hrelvo"/>
		<typeAlias type="kr.co.yamsuleng.mvc.dto.HashTagVO"  alias="hvo"/>
		
		<typeAlias type="kr.co.yamsuleng.mvc.dto.Criteria"  alias="cri"/>
		
		<typeAlias type="kr.co.yamsuleng.mvc.dto.FollowVO"  alias="folvo"/>
		
		<typeAlias type="kr.co.yamsuleng.mvc.dto.StoreVO"  alias="strvo"/>
	</typeAliases>
</configuration>  